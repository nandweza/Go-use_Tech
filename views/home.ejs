  <%- include('./partials/head.ejs') %>

  <body>
    <%- include('./partials/nav.ejs') %>
      <div class="main">
        <div class="main-home">
          <h1 class="text">GO USETECH</h1>
          <P class="text">
            Welcome to our education and tech website, where we strive<br>
            to provide you with the latest and most informative content<br>
            about the intersection of technology and education.
          </P>
          <div class="home-btn">
            <button class="main-btn left">
              <a href="/about">ABOUT US</a>
            </button>
            <button class="main-btn right">
              <a href="/register">GET STARTED</a>
            </button>
          </div>
        </div>
      </div>

    <div class="statements container">
      <!-- <h3 class="text-center">Our Slogan</h3> -->
      <div class="d-flex justify-content-center align-items-center pb-2">
        <i class="fa fa-quote-left"></i>
      </div>
      <p class="text-center pb-5">"Data-driven transformation, changing lives where it's needed most."</p>
      <div class="row">
        <div class="col-lg-4 col-sm-12">
          <div class="d-flex justify-content-center align-items-center">
            <i class="fa fa-eye"></i>
          </div>
          <h3 class="text-center mt-4">Our Vision</h3>
          <p class="text-center mt-2 stext">
            To become regional leader in consultancy, innovative
            market research and advanced analytical solutions.
          </p>
        </div>
        <div class="col-lg-4 col-sm-12">
          <div class="d-flex justify-content-center align-items-center">
            <i class="fa fa-bullseye"></i>
          </div>
          <h3 class="text-center mt-4">Our Mission</h3>
          <p class="text-center stext">
            Our mission is to empower individuals to explore,
            analyze, and understand their data through relevant
            and practical education, fostering data literacy for all
          </p>
        </div>
        <div class="col-lg-4 col-sm-12">
          <div class="d-flex justify-content-center align-items-center">
            <i class="fa fa-key"></i>
          </div>
          <h3 class="text-center mt-4">Our Core Values</h3>
          <p class="text-center stext">
            Excellence, Innovation, Leadership, Respect, Integrity, Diversity
          </p>
        </div>
      </div>
    </div>

    <div class="featured-courses">
      <div class="container">  
        <h2 class="mb-5 pt-5 featured-title">Featured Courses</h2>
        <div class="row">
         
        </div>
      </div>
    </div>

    <div class="featured-blog">
      <div class="container">
        <div class="row">
          <div class="col-lg-8">
            <div class="card p-5">
              <h3 class="mt-4 mb-4">Latest Posts</h3>
              <% if (posts.length > 0) { %> 
                <% posts.forEach((post) => { %>
                  <div class="row mb-4 pb-4 border-bottom">
                    <div class="col-lg-6 col-md-12">
                      <img src="<%= post.blogimg %>" class="img-responsive fblog-img" width="300" height="185">
                    </div>
                    <div class="col-lg-6 col-md-12">
                      <div class="fblog-title">
                        <%= post.title %>
                      </div>
                      <div class="fblog-desc">
                        <p>
                          <%= post.content.substring(0, 240) + "..." %>
                          <a href="/blog/<%= post._id %>" style="color: red;">Read more.</a>
                        </p>
                      </div>
                    </div>
                  </div>
                  <% }) %>
                <% } else { %>
                  <span class="text-center fw-bold" style="font-size: 18px;">No Post yet.</span>
                <% } %>
            </div>
          </div>
          <div class="col-lg-4 events">
            <div class="card p-3">
              <h3 class="mt-5">Schedule Event</h3>
              <div>
                <!-- Calendly inline widget begin -->
                <div class="calendly-inline-widget" data-url="https://calendly.com/gousetech" style="min-width:320px;height:490px;"></div>
                <script type="text/javascript" src="https://assets.calendly.com/assets/external/widget.js" async></script>
                <!-- Calendly inline widget end -->
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
